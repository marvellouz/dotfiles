filetype indent on
filetype plugin on
syntax on
set autochdir
let loaded_matchparen = 1
set autoindent
filetype plugin indent on
set smartindent
set backup
set softtabstop=2
set incsearch
set hlsearch
set ignorecase

" set shiftwidth=2
" set tabstop=8
" set expandtab
set backspace=indent,eol,start
set shiftwidth=2
colorscheme  Digerati_Vim_Colorscheme_by_hcalves
let loaded_matchparen = 1
set clipboard=unnamed
let g:EclimHome = '/usr/share/vim/vimfiles/eclim'
let g:EclimEclipseHome = '/usr/share/eclipse'
set nocompatible
set encoding=utf8
